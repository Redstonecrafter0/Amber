			"description": "A simple and extensive client base.",
			"contributors": {
				"Redstonecrafter0": "Owner"
			},
			"contact": {
				"homepage": "https://redstonecrafter0.github.io/Amber",
				"issues": "https://github.com/Redstonecrafter0/Amber/issues",
				"sources": "https://github.com/Redstonecrafter0/Amber"
			},
			"icon": "assets/example_mod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
